//excessão dos erros

public int linhas = 0;











//erro de E
public void setEntradas(string E) throws Exception
{
if(E >= 2)
throw new Exception("Há mais de uma entrada no seu labirinto, labirinto inválido");
}

//erro de S 
public void setSaidas (string S) throw Exception
{
if(S >= 2)
throw new Exception("Há mais de uma saida no seu labirinto, labirinto inválido");
}

//coisinhas são todos os caracteres aceitos no txt para o labirinto
